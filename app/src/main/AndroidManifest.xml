<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.researchapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ResearchApp">
        <activity android:name=".User.BarcodeActivity"></activity>
        <activity android:name=".User.NoteActivity" />
        <activity android:name=".User.ConferanceActivity" />
        <activity android:name=".User.JournalActivity" />
        <activity android:name=".RetriveFileActivity" />
        <activity android:name=".UploadFileActivity" />
        <activity android:name=".User.ResetPasswordActivity" />
        <activity android:name=".User.LoginAdminActivity" />
        <activity android:name=".User.SiignUpActivity" />
        <activity android:name=".ResearchUserActivity" />
        <activity android:name=".ResearchAdminActivity" />
        <activity
            android:name=".ChooserActivity"
            android:theme="@style/AppTheme1" />
        <activity android:name=".DashBoardActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>